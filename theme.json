{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [ "page", "post" ],
			"title": "Blank"
		},
		{
			"name": "404",
			"postTypes": [ "page" ],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "var(--wp--custom--wsu-color-gray--0)",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "var(--wp--custom--wsu-color-gray--85)",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "var(--wp--custom--wsu-color-crimson--crimson)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "var(--wp--custom--wsu-color-crimson--crimson-light)",
					"name": "Primary Light",
					"slug": "primary-light"
				},
				{
					"color": "var(--wp--custom--wsu-color-gray--70)",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "var(--wp--custom--wsu-color-gray--10)",
					"name": "Secondary Light",
					"slug": "secondary-light"
				}
			]
		},
		"custom": {
			"wsuColorAccent": {
				"autumn": "#ff6727",
				"goldfinch": "#f3e700",
				"vineyard": ":#aadc24",
				"pacificsky": "#5bc3f5",
				"midnight": "#002d61"
			},
			"wsuColorCrimson": {
				"crimson": "#a60f2d",
				"crimson-light": "#ca1237",
				"crimson-accent": "#fae6ea",
				"crimson-dark": "#78001e"
			},
			"wsuColorGray": {
				"0": "hsl(0, 0%, 100%)",
				"5": "hsl(0, 0%, 95%)",
				"10": "hsl(0, 0%, 90%)",
				"15": "hsl(0, 0%, 85%)",
				"20": "hsl(0, 0%, 80%)",
				"25": "hsl(0, 0%, 75%)",
				"30": "hsl(0, 0%, 70%)",
				"35": "hsl(0, 0%, 65%)",
				"40": "hsl(0, 0%, 60%)",
				"45": "hsl(0, 0%, 55%)",
				"50": "hsl(0, 0%, 50%)",
				"55": "hsl(0, 0%, 45%)",
				"60": "hsl(0, 0%, 40%)",
				"65": "hsl(0, 0%, 35%)",
				"70": "hsl(0, 0%, 30%)",
				"75": "hsl(0, 0%, 25%)",
				"80": "hsl(0, 0%, 20%)",
				"85": "hsl(0, 0%, 15%)",
				"90": "hsl(0, 0%, 10%)",
				"95": "hsl(0, 0%, 3%)",
				"100": "hsl(0, 0%, 3%)"
			}
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "1200px"
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": {
				"minFontSize": "11px"
			},
			"fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontDisplay": "block",
                            "fontFamily": "IBM Plex Mono",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "300",
                            "src": [
                                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Light.woff2"
                            ]
                        },
                        {
                            "fontDisplay": "block",
                            "fontFamily": "IBM Plex Mono",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Regular.woff2"
                            ]
                        },
                        {
                            "fontDisplay": "block",
                            "fontFamily": "IBM Plex Mono",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "400",
                            "src": [
                                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Italic.woff2"
                            ]
                        },
                        {
                            "fontDisplay": "block",
                            "fontFamily": "IBM Plex Mono",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Bold.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "'IBM Plex Mono', monospace",
                    "name": "IBM Plex Mono",
                    "slug": "ibm-plex-mono"
                },
                {
                    "fontFace": [
                        {
                            "fontFamily": "Montserrat",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/montserrat/Montserrat-VariableFont_wght.ttf.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Montserrat",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/montserrat/Montserrat-Italic-VariableFont_wght.ttf.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "Montserrat, serif",
                    "name": "Montserrat",
                    "slug": "montserrat"
                }
			],
			"fontSizes": [
				{
					"size": "0.875rem",
					"fluid": {
						"min": "0.75rem",
						"max": "0.875rem"
					},
					"slug": "small"
				},
				{
					"size": "1.125rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"slug": "medium"
				},
				{
					"size": "1.5rem",
					"fluid": {
						"min": "1.125rem",
						"max": "1.5rem"
					},
					"slug": "large"
				},
				{
					"size": "3.125rem",
					"fluid": {
						"min": "2.34375rem",
						"max": "3.125rem"
					},
					"slug": "x-large"
				},
				{
					"size": "5.125rem",
					"fluid": {
						"min": "4.125rem",
						"max": "5.125rem"
					},
					"slug": "xx-large"
				}
			]
		},
        "useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"h1": {
				"spacing": {
					"margin": {
						"bottom": "3.125rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.2",
					"fontWeight": "800"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(1.5rem, 1.5rem + ((1vw - 0.48rem) * 0.962), 2rem)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(0.9375rem, 0.059rem + ((1vw - 7.68px) * 2.291), 1.25rem)"
				}
			},
			"h5": {
				"spacing": {
					"margin": {
						"bottom": "0.5rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"spacing": {
					"margin": {
						"bottom": "0.5rem"
					}
				},
				"typography": {
					"fontSize": "1rem"
				}
			},
			"heading": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0.75rem"
					},
					"padding": {
						"top": "0.25rem"
					}
				},
				"typography": {
					"fontWeight": "600",
					"lineHeight": "1.4"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		}
	]
}
